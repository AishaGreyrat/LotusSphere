doctype html
html
  head
    title LotusSphere
    meta(charset='UTF-8')
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel='stylesheet', href='/css/styles.css')
    link(rel="icon" href="/images/titulologomorado.png" type="image/x-icon")
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous")
  body.signika-negative-font
    section
      .container.py-5
        .row
          .col
            nav(aria-label="breadcrumb" class="navConfig rounded-3 p-3 mb-4")
              ol.breadcrumb.mb-0
                li.breadcrumb-item 
                  a(class="logo" href="/")
                    img(src="/images/logosimplemorado.png" width="70" height="50" class="d-inline-block align-top" alt="")
                li.breadcrumb-item: a.itemConf(href="/") Home
                if !user
                  li.breadcrumb-item: a.itemConf(href="/login") Login
                  li.breadcrumb-item: a.itemConf(href="/register") Register
                else
                  li.breadcrumb-item: a.itemConf(href="/proyectos") mis Proyectos
                  li.breadcrumb-item: a.itemConf(href="/profile") Perfil del usuario
                  li.breadcrumb-item: a.itemConf(href="/logout") Logout
        block content

script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous")
